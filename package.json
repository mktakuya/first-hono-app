  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.0",
    "wrangler": "^3.1.0"
  }
}